# https://kafka.apache.org/
kafka_run_class: ~  # kafka-run-class.sh
kafka_server_start: ~  # kafka-server-start.sh

# kinit, Kerberos distribution
kinit: ~
klist: ~

# https://github.com/elastic/logstash-forwarder
logstash-forwarder: ~

# https://www.isc.org/downloads/bind/
named: /usr/sbin/named

# http://oss.oetiker.ch/rrdtool/
rrdcached: ~  # rrdcached
rrdtool: ~  # rrdtool

# http://skarnet.org/software/s6/
s6-log: /bin/s6-log
s6-svc: /bin/s6-svc
s6-svok: /bin/s6-svok
s6-svscan: /bin/s6-svscan
s6-svscanctl: /bin/svscanctl
s6-svwait: /bin/svwait
s6: /bin/s6

# Treadmill utilities.
pid1: /home/centos/treadmill-pid1/pid1
tkt-recv: ~
tkt-send: ~

# Shared libraries.
#
# ld_preload libs use $LIB notation, and in the code should be resolved with
# check=False.
treadmill_bind_preload.so: ~
